<h3>Cliente</h3>
<form class="form-horizontal" role="form">
	<div class="form-group">
		<label for="dni" class="col-sm-3 control-label">DNI</label>
		<div class="col-sm-8">
			<input type="text" class="form-control" name="dni_cuil" value="{{dni_cuil}}">
		</div>
	</div>
	<div class="form-group">
		<label for="apellido" class="col-sm-3 control-label">Denominación</label>
		<div class="col-sm-8">
			<input type="text" class="form-control" name="denomination" value="{{denomination}}">
		</div>
	</div>
	<div class="form-group">
		<label for="dni" class="col-sm-3 control-label">Teléfono</label>
		<div class="col-sm-8">
			<input type="text" class="form-control" name="phone" value="{{phone}}">
		</div>
	</div>
	<div class="form-group">
		<label for="dni" class="col-sm-3 control-label">Dirección</label>
		<div class="col-sm-8">
			<input type="text" class="form-control" name="address" value="{{address}}">
		</div>
	</div>
	<div class="form-group">
		<label for="dni" class="col-sm-3 control-label">E-Mail</label>
		<div class="col-sm-8">
			<input type="text" class="form-control" name="email" value="{{email}}">
		</div>
	</div>
</form>