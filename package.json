      "aleph-frontend/stock/nuevo_remito/",
      "aleph-frontend/stock/registros_articulo/",
      "aleph-frontend/stock/registros_remito/",
      "aleph-frontend/stock/reportes/",
      "aleph-frontend/ventas/main.component!",
