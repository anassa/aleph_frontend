      "aleph-frontend/ventas/nuevo_cliente/",
      "aleph-frontend/ventas/registros_forma_de_pago/",
      "aleph-frontend/ventas/registros_solicitud/",
      "aleph-frontend/ventas/registros_solicitud_venta/",
      "aleph-frontend/ventas/registros_venta/",
      "aleph-frontend/ventas/registros_cliente/",
      "aleph-frontend/ventas/reportes/"
